<!Doctype html>
<html>

<head>
    <title>Management Console</title>
    <link href='/css/style.css' rel='stylesheet' type='text/css' />
</head>

<body>
    <div class="fullcontainer">
        <div id="solidlinecontainer">
            <div id="solidline">
                <div id="bslogo"></div>
                <div id="title">
                    <b>
                    <p>Brightside Community Homes Foundation</p>
                    <p>Database Management Console</p>
                    </b>
                </div>
            </div>
        </div>
        <div>
            <div id="cpanel">
                <div id="cpanelin">
                    <p><b id="summary">Summary</b></pid>
                    <p><b id="add">Add Data</b></p>
                </div>
            </div>

            {{!-- information panel. shows either summary or add data. --}}
            {{!-- summary --}}
            <div id="infopanel">
                <div id="tablecontainer">
                    <table style="width:100%">
                        <thead style="background-color: #401e6c; color: white;">
                            <col >
                            <col >
                            <col width='30%'>
                            <col >
                            <col >
                            <col >
                            <col >
                            <col width='11%'>
                            <col width='5%'>
                            <col >
                            <tr>
                                <th>Location</th>
                                <th>Organization</th>
                                <th>Description</th>
                                <th>Keywords</th>
                                <th>Contact</th>
                                <th>Toll-Free</th>
                                <th>Website</th>
                                <th>Hours</th>
                                <th>Perks</th>
                                <th>Type</th>
                                <th>Delete?</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each data}}
                                <tr id={{resourceId}}>

                                    {{#if location}}
                                        <td>{{location}}</td>
                                    {{else}}
                                        <td>Phone Only</td>
                                    {{/if}}

                                    <td>{{organization}}</td>

                                    <td>{{description}}</td>

                                    <td>
                                        {{#each keywords}}
                                            <p>{{this}}</p>
                                        {{/each}}
                                    </td>

                                    <td>{{phoneNumber}}</td>

                                    {{#if tollFree}}
                                        <td>{{tollFree}}</td>
                                    {{else}}
                                        <td>N/A</td>
                                    {{/if}}

                                    <td>{{website}}</td>

                                    {{#if hours}}
                                        <td>
                                            {{#each hours}}
                                                <p>{{this}}</p>
                                            {{/each}}
                                        </td>
                                    {{else}}
                                        <td>No Data Available</td>
                                    {{/if}}

                                    <td>
                                        {{#each perks}}
                                            <p>{{this}}</p>
                                        {{/each}}
                                    </td>

                                    <td>{{#each type}}
                                            <p>{{this}}</p>
                                        {{/each}}
                                    </td>

                                    <td>
                                        <form action='manage/delete' method="POST" onsubmit="return confirm('Are you sure you want to delete this entry?');">
                                            <input style="display: none" type="text" name="resId" value={{resourceId}} />
                                            <input id="delButton" type="image" src="/img/delete.png" />
                                        </form>
                                    </td>
                                </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div>
            </div>

            {{!-- add data --}}
            <div id="addpanel">
                <h3>NOTE: leave inputs blank for the following cases
                    <h4>
                        <ul>• Locations that have no physical address ie. Phone or web only</ul>
                        <ul>• No toll free numbers</ul>
                        <ul>• No websites</ul>
                    </h4>
                </h3>
                <form action='manage/submit' method="POST" onsubmit="return confirm('Do you want to add this data to database?');">
                    <div>
                        <div style='display: inline-block;'>
                            <p id="redText"><b id="blackBold">Location:</b> *optional</p>
                            <input class='inputstyle' type='text' name='location'>
                        </div>

                        <div style='display: inline-block; margin-left: 5vw;'>
                            <p id="redText"><b id="blackBold">Organization: </b>*required</p>
                            <input class='inputstyle' type='text' name='organization' required>
                        </div>
                    </div>

                    <p id="redText"><b id="blackBold">Description: </b>*required</p>
                    <textarea name='description' style='border-radius: 5px;' required></textarea>

                    <div>
                        <div style='display: inline-block;'>
                            <p id="redText"><b id="blackBold">Contact: </b>*required</p>
